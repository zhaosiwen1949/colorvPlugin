{
    "manifest_version": 2,
    "name": "Save the video",
    "version": "1.0",
    "description": "Save the video made by colorv",
    "background":{
    	"scripts":[
    		"background.js"
    	],
    	"persistent":false
    },
    "content_scripts":[
    	{
    		"matches":["*://video.colorv.cn/play/*"],
    		"js":["jquery-3.2.1.min.js","main.js"]
    	}
    ],
    "permissions": [
        "downloads"
    ]
}
